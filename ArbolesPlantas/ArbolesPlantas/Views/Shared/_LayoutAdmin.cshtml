						<a href="#" class="nav-link dropdown-toggle" id="proveedores" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
							<i class=" fas fa-user-circle"></i>Compras
						</a>
						<div class="dropdown-menu" aria-labelledby="proveedores">
							<a href="@Url.Action("Index","Proveedores")" class="dropdown-item">Ver Proveedores</a>
